 "format_data": "[{\"fieldname\": \"print_heading_template\", \"fieldtype\": \"Custom HTML\", \"options\": \"<style>\\nh2.background {\\n\\t position: relative;\\n\\t z-index: 1;\\n}\\n h2.background:before {\\n\\t content: \\\"\\\";\\n\\t margin: 0 auto;\\n\\t position: absolute;\\n\\t top: 25%;\\n\\t left: 0;\\n\\t right: 0;\\n\\t bottom: 0;\\n\\t width: 95%;\\n\\t z-index: -1;\\n     height: 20px;\\n     background: #ffc809;\\n}\\n h2.background span {\\n\\t background: #fff;\\n\\t padding: 0 15px;\\n\\t margin-right: 70px;\\n}\\n .print-heading{\\n     border: none !important;\\n }\\n</style>\\n<div class=\\\"print-heading\\\">\\n    <h2 class=\\\"background\\\">\\n       <span>Invoice</span>\\n    </h2>\\n</div>\"}, {\"fieldtype\": \"Section Break\", \"label\": \"\"}, {\"fieldtype\": \"Column Break\"}, {\"fieldname\": \"_custom_html\", \"print_hide\": 0, \"label\": \"Custom HTML\", \"fieldtype\": \"HTML\", \"options\": \"<div class=\\\"row\\\">\\n    <div class=\\\"col-xs-4 column-break text-left\\\">\\n    <div>\\n        <p style=\\\"font-size: 25px\\\"><b>{{ _(\\\"Invoice To\\\")+\\\" :\\\" }}</b></p>\\n        <p style=\\\"font-size: 25px\\\"><b>{{ doc.customer_name }}</b></p>\\n    </div>\\n</div>\\n<div class=\\\"col-xs-4 column-break\\\">\\n    <div class=\\\"text-center\\\">\\n        {% if doc.gsg_qrcode %}\\n        <img width=\\\"150px\\\" src=\\\"{{ doc.gsg_qrcode }}\\\">\\n        {% endif %}\\n    </div>\\n</div>\\n<div class=\\\"col-xs-4 column-break text-right\\\">\\n    <div>\\n        <p style=\\\"font-size: 15px\\\"><b>{{ _(\\\"Invoice\\\")+\\\"#\\\" }} {{ doc.name }}</b></p>\\n        <p style=\\\"font-size: 15px\\\"><b>{{ _(\\\"Date\\\") }} {{ doc.posting_date }}</b></p>\\n    </div>\\n</div>\\n\\n</div>\"}, {\"fieldtype\": \"Section Break\", \"label\": \"Items\"}, {\"fieldtype\": \"Column Break\"}, {\"fieldname\": \"_custom_html\", \"print_hide\": 0, \"label\": \"Custom HTML\", \"fieldtype\": \"HTML\", \"options\": \"<div data-fieldname=\\\"items\\\" data-fieldtype=\\\"Table\\\">\\n  <table class=\\\"table table-condensed table-striped\\\">\\n    <thead class=\\\"thead-dark\\\">\\n      <tr>\\n        <th\\n            style=\\\"width: 40px; background-color: #333C44 !important; color: #FFF;\\\"\\n            class=\\\"table-sr text-center\\\">{{ _(\\\"SL\\\") }}\\n        </th>\\n        <th\\n          style=\\\"width: 150px; background-color: #333C44 !important; color: #FFF;\\\"\\n          data-fieldname=\\\"items\\\"\\n          data-fieldtype=\\\"Table\\\"\\n        >\\n          {{ _(\\\"Item Description\\\") }}\\n        </th>\\n\\t\\t<th\\n          style=\\\"width: 80px; background-color: #333C44 !important; color: #FFF;\\\"\\n          class=\\\"text-center\\\"\\n          data-fieldname=\\\"items\\\"\\n          data-fieldtype=\\\"Table\\\"\\n        >\\n          {{ _(\\\"Price\\\") }}\\n        </th>\\n        <th\\n          style=\\\"width: 80px; background-color: #333C44 !important; color: #FFF;\\\"\\n          class=\\\"text-center\\\"\\n          data-fieldname=\\\"items\\\"\\n          data-fieldtype=\\\"Table\\\"\\n        >\\n          {{ _(\\\"Qty\\\")+\\\".\\\" }}\\n        </th>\\n        <th\\n          style=\\\"width: 80px; background-color: #333C44 !important; color: #FFF;\\\"\\n          class=\\\"text-center\\\"\\n          data-fieldname=\\\"items\\\"\\n          data-fieldtype=\\\"Table\\\"\\n        >\\n          {{ _(\\\"Total\\\") }}\\n        </th>\\n      </tr>\\n    </thead>\\n    <tbody>\\n\\t\\t{% for item in doc.items %}\\n      <tr>\\n        <td class=\\\"table-sr\\\">{{item.idx}}</td>\\n        <td\\n          data-fieldname=\\\"items\\\"\\n          data-fieldtype=\\\"Table\\\">\\n          <div class=\\\"value\\\">\\n\\t\\t\\t{{item.description}}\\n          </div>\\n        </td>\\n        <td class=\\\"text-center\\\" data-fieldname=\\\"items\\\" data-fieldtype=\\\"Table\\\">\\n          <div class=\\\"value\\\">\\n\\t\\t\\t{{item.get_formatted(\\\"rate\\\", doc)}}\\n          </div>\\n        </td>\\n        <td class=\\\"text-center\\\" data-fieldname=\\\"items\\\" data-fieldtype=\\\"Table\\\">\\n          <div class=\\\"value\\\">{{item.qty}}</div>\\n        </td>\\n        <td class=\\\"text-center\\\" data-fieldname=\\\"items\\\" data-fieldtype=\\\"Table\\\">\\n          <div class=\\\"value\\\">{{item.get_formatted(\\\"amount\\\", doc)}}</div>\\n        </td>\\n      </tr>\\n\\t  {% endfor %}\\n    </tbody>\\n  </table>\\n</div>\\n\"}, {\"fieldtype\": \"Section Break\", \"label\": \"\"}, {\"fieldtype\": \"Column Break\"}, {\"fieldname\": \"_custom_html\", \"print_hide\": 0, \"label\": \"Custom HTML\", \"fieldtype\": \"HTML\", \"options\": \"<h3>Thank you for your busniess</h3>\\n<h5>Terms and Conditions Details</h5>\"}, {\"fieldname\": \"terms\", \"print_hide\": 0, \"align\": \"left\", \"label\": \"Terms and Conditions Details\"}, {\"fieldtype\": \"Column Break\"}, {\"fieldname\": \"total_taxes_and_charges\", \"print_hide\": 0, \"label\": \"Total Taxes and Charges\"}, {\"fieldname\": \"grand_total\", \"print_hide\": 0, \"label\": \"Grand Total\"}]",
 "html": "<style>\nh2.background {\n\t position: relative;\n\t z-index: 1;\n\t margin: 0 0 25px 0;\n}\n h2.background:before {\n\t content: \"\";\n\t margin: 0 auto;\n\t position: absolute;\n\t top: 0;\n\t left: 0;\n\t right: 0;\n\t bottom: 0;\n\t width: 95%;\n\t z-index: -1;\n     height: 30px;\n     background: #ffc809;\n}\n h2.background span {\n\t background: #fff;\n\t padding: 0 15px;\n\t margin-right: 70px;\n\t font-size: 40px;\n     text-transform: uppercase;\n}\n .print-heading{\n     border: none !important;\n }\n</style>\n<div class=\"row section-break\">\n   <h2 class=\"background text-right\">\n      <span>Invoice</span>\n   </h2>\n</div>\n<div class=\"row section-break\" data-label=\"\">\n   <div class=\"col-xs-12 column-break\">\n      <div>\n         <div class=\"row\">\n            <div class=\"col-xs-4 column-break text-left\">\n               <div>\n                  <p style=\"font-size: 20px\"><b>Invoice To :</b></p>\n                  <p style=\"font-size: 20px\"><b>{{ doc.customer }}</b></p>\n                  <p style=\"font-size: 15px; color:gray\">{{ doc.address_display }}</p>\n               </div>\n            </div>\n            <div class=\"col-xs-4 column-break\">\n               <div class=\"text-center\">\n                  <img style=\"width: 120px;\" src=\"{{ doc.gsg_qrcode }}\">\n               </div>\n            </div>\n            <div class=\"col-xs-4 column-break text-left\">\n                   <div class=\"row\" style=\"margin-bottom: 10px; margin-top: 30px\">\n                        <div class=\"col-xs-4 text-left\" style=\"font-size: 15px; padding: 0\">\n                            <b>Invoice#</b>\n                        </div>\n                        <div class=\"col-xs-8 text-left\" style=\"font-size: 15px; color:gray\">\n                            <b>{{ doc.name }}</b>\n                        </div>\n                   </div>\n                   <div class=\"row\">\n                        <div class=\"col-xs-4 text-left\" style=\"font-size: 15px; padding: 0\">\n                            <b>Date</b>\n                        </div>\n                        <div class=\"col-xs-8 text-left\" style=\"font-size: 15px; color:gray\">\n                            <b>{{ doc.posting_date }}</b>\n                        </div>\n                   </div>\n            </div>\n         </div>\n      </div>\n   </div>\n</div>\n<div class=\"row section-break\" data-label=\"Items\">\n   <div class=\"col-xs-12 column-break\">\n      <div>\n         <div data-fieldname=\"items\" data-fieldtype=\"Table\">\n            <table class=\"table table-condensed table-striped\">\n               <thead class=\"thead-dark\">\n                  <tr>\n                     <th style=\"width: 40px; background-color: #333C44 !important; color: #FFF;\" class=\"table-sr text-center\">SL\n                     </th>\n                     <th style=\"width: 150px; background-color: #333C44 !important; color: #FFF;\" data-fieldname=\"items\" data-fieldtype=\"Table\">\n                        Item Description\n                     </th>\n                     <th style=\"width: 80px; background-color: #333C44 !important; color: #FFF;\" class=\"text-center\" data-fieldname=\"items\" data-fieldtype=\"Table\">\n                        Price\n                     </th>\n                     <th style=\"width: 80px; background-color: #333C44 !important; color: #FFF;\" class=\"text-center\" data-fieldname=\"items\" data-fieldtype=\"Table\">\n                        Qty.\n                     </th>\n                     <th style=\"width: 80px; background-color: #333C44 !important; color: #FFF;\" class=\"text-center\" data-fieldname=\"items\" data-fieldtype=\"Table\">\n                        Total\n                     </th>\n                  </tr>\n               </thead>\n               <tbody>\n                 {% for item in doc.items %}\n                  <tr>\n                     <td class=\"table-sr\">{{ item.idx }}</td>\n                     <td data-fieldname=\"items\" data-fieldtype=\"Table\">\n                        <div class=\"value\">\n                           {{ item.description }}\n                        </div>\n                     </td>\n                     <td class=\"text-center\" data-fieldname=\"items\" data-fieldtype=\"Table\">\n                        <div class=\"value\">\n                            {{ item.get_formatted(\"rate\", doc) }}\n                        </div>\n                     </td>\n                     <td class=\"text-center\" data-fieldname=\"items\" data-fieldtype=\"Table\">\n                        <div class=\"value\">{{ item.qty }}</div>\n                     </td>\n                     <td class=\"text-center\" data-fieldname=\"items\" data-fieldtype=\"Table\">\n                        <div class=\"value\">\n                            {{ item.get_formatted(\"amount\", doc) }}\n                        </div>\n                     </td>\n                  </tr>\n                  {% endfor %}\n               </tbody>\n            </table>\n         </div>\n      </div>\n   </div>\n</div>\n<div class=\"row section-break\" data-label=\"\">\n   <div class=\"col-xs-6 column-break\">\n      <div>\n         <h3 style=\"margin: 0\">Thank you for your busniess</h3>\n         <h5>Terms and Conditions Details</h5>\n         <p>{{ doc.terms }}</p>\n      </div>\n   </div>\n   <div class=\"col-xs-6 column-break\">\n       <div class=\"row important data-field\" style=\"font-size: 15px;\" data-fieldname=\"total\" data-fieldtype=\"Currency\">\n         <div class=\"col-xs-5\">\n            <label>Sub Total: </label>\n         </div>\n         <div class=\"col-xs-7 text-right value\">\n            {{ doc.get_formatted(\"total\") }}\n         </div>\n      </div>\n      <div class=\"row  data-field\" style=\"font-size: 15px;\" data-fieldname=\"total_taxes_and_charges\" data-fieldtype=\"Currency\">\n         <div class=\"col-xs-5\">\n            <label>Tax</label>\n         </div>\n         <div class=\"col-xs-7 text-right value\">\n             {{ doc.get_formatted(\"total_taxes_and_charges\") }}\n         </div>\n      </div>\n      <div class=\"row important data-field\" style=\"font-size: 15px;background: #ffc809;padding:5px\" data-fieldname=\"grand_total\" data-fieldtype=\"Currency\">\n         <div class=\"col-xs-5\">\n            <label>Total</label>\n         </div>\n         <div class=\"col-xs-7 text-right value\">\n            {{ doc.get_formatted(\"grand_total\") }}\n         </div>\n      </div>\n   </div>\n</div>",
